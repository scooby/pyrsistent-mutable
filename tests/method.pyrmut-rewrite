
from pyrsistent_mutable.globals import invoke as _invoke, set_via_attr as _set_via_attr

def simple_invoke(value):
    value = _invoke(value, 'append', 20)
    return value

def complex_invoke(value):
    value = _set_via_attr(value, 'foo', _invoke(value.foo, 'extend', [20, 30, 40]))
    return value
